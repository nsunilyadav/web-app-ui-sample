{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components": ["./components"],
      "@components/*": ["./components/*"],
      "@constants": ["./constants"],
      "@constants/*": ["./constants/*"],
      "@layouts": ["./layouts"],
      "@layouts/*": ["./layouts/*"],
      "@pages": ["./pages"],
      "@pages/*": ["./pages/*"],
      "@routes": ["./routes"],
      "@routes/*": ["./routes/*"],
      "@services": ["./services"],
      "@services/*": ["./services/*"],
      "@utils": ["./utils"],
      "@utils/*": ["./utils/*"],
      "@assets": ["./assets"],
      "@assets/*": ["./assets/*"]
    }
  }
}
